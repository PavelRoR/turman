#results
	padding 50px 0 0
.result
	margin-bottom: 40px
	&-title
		display: inline-block
		font-size: 50px
		font-family: PTSB
		position relative
		line-height: 54px
		margin-bottom: 11px
		&:before
			content: ''
			// display: block
			width: 100%
			height 3px
			background-color #FF1516
			left 0
			bottom 0
			position absolute